<div class="center tdDiv">
    
<!--Elegir nombre del producto-->
<label for="name"style="padding: 5px; width: 100%; text-align:center;">Elegí el nombre del producto:</label> <br>
<input type="text" style="padding: 5px; width: 50%; text-align:center; border-color: rgba(0, 0, 0, 1); border-width: 0.1px; margin: auto;" name="name" <% if (typeof productToEdit != 'undefined' ) { %> value="<%= productToEdit.name %>" <% } else { %> placeholder="Escribí aquí el nombre del producto" <% } %> required> <br>
 
<!--Elegir tipo de producto-->
<label for="type" style="padding: 5px; width: 100%; text-align:center;">Elegí el tipo de producto:</label> <br>
<input type="text" style="padding: 5px; width: 50%; text-align:center; border-color: rgba(0, 0, 0, 1); border-width: 0.1px; margin: auto;" name="type" <% if (typeof productToEdit != 'undefined' ) { %> value="<%= productToEdit.type %>" <% } else { %> placeholder="Escribí aquí el nombre del producto sin mayúsculas ni espacios" <% } %> required>

<!--Elegir imagen del producto-->
<label style="display: block; padding: 5px; width: 100%; text-align:center;margin-top: 1%;" for="image">Elegí una imagen que describa el producto:</label>
<input style="margin: 1% auto" type="file" name="image" id="image" required>

</div>

<section class="banner1">
